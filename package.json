{
    "name": "crypto-price-fetcher",
    "version": "1.0.0",
    "description": "Fetch real-time cryptocurrency prices from DEX pools via public RPCs",
    "main": "./dist/index.js",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "import": "./dist/index.mjs",
            "require": "./dist/index.js",
            "types": "./dist/index.d.ts"
        }
    },
    "files": [
        "dist",
        "README.md"
    ],
    "scripts": {
        "build": "bun build ./src/index.ts --outdir ./dist --target node && tsc --emitDeclarationOnly",
        "typecheck": "tsc --noEmit",
        "prepublishOnly": "bun run build"
    },
    "keywords": [
        "crypto",
        "price",
        "ethereum",
        "solana",
        "bsc",
        "base",
        "uniswap",
        "pancakeswap",
        "dex",
        "defi"
    ],
    "author": "",
    "license": "MIT",
    "dependencies": {
        "viem": "^2.0.0"
    },
    "devDependencies": {
        "@types/bun": "latest",
        "typescript": "^5.0.0"
    },
    "peerDependencies": {
        "viem": ">=2.0.0"
    }
}